

  <div class="container my-5">
      
<table class="table table-striped">
  <tr>
      <th>Name</th>
      <th>Email</th>
      <th>City</th>
      <th>Actions</th>
      
  </tr>
  <tbody>
{{#each obj}}

<tr>
    <td>{{this.name}}</td>
    <td>{{this.email}}</td>
    <td>{{this.city}}</td>
    <td><button class="btn btn-primary" onclick="location.replace('/upd/edt/{{this.email}}')">Edit</button>
    <button class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#{{this.name}}">Delete</button></td>
 
</tr>
  
{{/each}}
  </tbody>
 
</table>
 <hr>
  <button class="btn btn-secondary" onclick="location.replace('/')">Add Users</button>

  </div>
{{#each obj}}
     <div class="modal" tabindex="-1" id="{{this.name}}">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Are you sure?</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <p>Modal body text goes here.</p>
      </div>
      <div class="modal-footer">
        <button  class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
        <button  class="btn btn-primary" onclick="location.replace('/delete/{{this.name}}')">Delete</button>
      </div>
    </div>
  </div>
</div>

  {{/each}}